{
  "name": "my-app",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun --bun vite dev --port 3000",
    "build": "bun --bun vite build",
    "preview": "bun --bun vite preview",
    "fmt": "oxfmt",
    "fmt:check": "oxfmt --check",
    "lint": "oxlint",
    "auth:generate": "bunx --bun @better-auth/cli@latest generate --output ./src/db/auth-schema.ts -y",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio",
    "admin:create": "bun scripts/manage-admin.ts create",
    "admin:change-password": "bun scripts/manage-admin.ts change-password",
    "ci-build": "bun install --frozen-lockfile && bun run build"
  },
  "dependencies": {
    "@base-ui/react": "^1.1.0",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@inquirer/prompts": "^8.2.0",
    "@libsql/client": "^0.17.0",
    "@t3-oss/env-core": "^0.13.8",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-devtools": "^0.7.0",
    "@tanstack/react-query": "^5.66.5",
    "@tanstack/react-query-devtools": "^5.84.2",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/react-router-ssr-query": "^1.131.7",
    "@tanstack/react-start": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "better-auth": "^1.4.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.8",
    "drizzle-orm": "^0.45.0",
    "lucide-react": "^0.563.0",
    "nitro": "npm:nitro-nightly@latest",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "shadcn": "^3.8.4",
    "shiki": "^3.22.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "^1.4.0",
    "universal-cookie": "^8.0.1",
    "vite-tsconfig-paths": "^6.0.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@tanstack/devtools-vite": "^0.3.11",
    "@types/bun": "latest",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "dotenv": "^16.0.0",
    "oxfmt": "^0.28.0",
    "oxlint": "^1.43.0",
    "typescript": "^5.7.2",
    "vite": "^7.1.7"
  }
}
